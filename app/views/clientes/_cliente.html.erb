<div id="<%= dom_id cliente %>">

  <p>
    <strong>Usuario ID:</strong>
    <%= cliente.usuario_id %>
  </p>

  <p>
    <strong>Nome:</strong>
    <%= cliente.nome %>
  </p>

  <p>
    <strong>Data de nascimento:</strong>
    <%= cliente.data_de_nascimento %>
  </p>

  <p>
    <strong>CPF:</strong>
    <%= cliente.cpf %>
  </p>

  <p>
    <strong>E-mail:</strong>
    <%= cliente.email %>
  </p>
</div>
